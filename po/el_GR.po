#: src\Sight.cpp.bak:868
#, c-format
msgid "ParallaxCorrection = -180/Pi * asin( sin(Pi/180 * HP ) * cos(Pi/180 * CorrectedAltitude))\n"
"ParallaxCorrection = -180/Pi * asin( sin(Pi/180 * %.4f ) * cos(Pi/180 * %.4f))\n"
"ParallaxCorrection = %.4f\n"
msgstr "ΔιόρθωσηΠαράλλαξης = -180/Pi * asin( sin(Pi/180 * HP ) * cos(Pi/180 * ΔιορθωμένοΥψόμετρο))\n"
"ΔιόρθωσηΠαράλλαξης = -180/Pi * asin( sin(Pi/180 * %.4f ) * cos(Pi/180 * %.4f)) = %.4f\n"

#: src\Sight.cpp:693 src\Sight.cpp.bak:694
#, c-format
msgid "\n"
"Observed Altitude (Ho)\n"
"ObservedAltitude = CorrectedAltitude - ParallaxCorrection\n"
"ObservedAltitude = %.4f - %.4f\n"
"ObservedAltitude = %.4f\n"
msgstr "\n"
"Παρατήρηση Υψομέτρου (Ho)\n"
"ΠαρατήρησηΥψομέτρου = ΔιορθωμένοΥψόμετρο - ΔιόρθωσηΠαράλλαξης\n"
"ΠαρατήρησηΥψομέτρου = %.4f - %.4f\n"
"ΠαρατήρησηΥψομέτρου = %.4f\n"

#: src\Sight.cpp:727 src\Sight.cpp.bak:728
#, c-format
msgid "\n"
"Apparent Moon Altitude (Ha)\n"
"ApparentAltitudeMoon = MeasurementMoon - IndexCorrection - EyeHeightCorrection\n"
"ApparentAltitudeMoon = %.4f - %.4f - %.4f\n"
"ApparentAltitudeMoon = %.4f\n"
msgstr "\n"
"Φαινόμενο ύψος σελήνης (Ha)\n"
"ΦαινΥψοςΣελήνης = ΜετρΣελήνης - ΔιορθΔείκτη - ΔιορθΥψουςΜατιού\n"
" ΦαινΥψοςΣελήνης = %.4f - %.4f - %.4f\n"
"ΦαινΥψοςΣελήνης = %.4f\n"

#: src\Sight.cpp:738 src\Sight.cpp.bak:739
#, c-format
msgid "\n"
"Refraction Correction\n"
"x = tan(Pi/180*ApparentAltitudeMoon + 4.848e-2*(Pi/180) / (tan(Pi/180*ApparentAltitudeMoon) + .028))\n"
"x = tan(Pi/180*%.4f + 4.848e-2*(Pi/180) / (tan(Pi/180*%.4f) + .028))\n"
"x = %.4f\n"
msgstr "\n"
"Διόρθωση διάθλασης\n"
"x = tan(π/180*ΦαινΥψοςΣελήνης + 0.04848*(π/180) / (tan(π/180*ΦαινΥψοςΣελήνης) + .028))\n"
"x = tan(Pi/180*%.4f + 0.04848*(π/180) / (tan(π/180*%.4f) + .028))\n"
"x = %.4f\n"

#: src\Sight.cpp:743 src\Sight.cpp.bak:744
#, c-format
msgid "RefractionCorrectionMoon = .267 * Pressure / (x*(Temperature + 273.15)) / 60.0\n"
"RefractionCorrectionMoon = .267 * %.4f / (x*(%.4f + 273.15)) / 60.0\n"
"RefractionCorrectionMoon = %.4f\n"
msgstr "ΔιόρθΔιαθλΣελήνης = 0.267 * Πίεση / (x*(Θερμοκρασία + 273.15)) / 60.0\n"
"ΔιόρθΔιαθλΣελήνης = 0.267 * %.4f / (x*(%.4f + 273.15)) / 60.0\n"
"ΔιόρθΔιαθλΣελήνης=%.4f\n"

#: src\Sight.cpp:768 src\Sight.cpp:771 src\Sight.cpp.bak:769
#: src\Sight.cpp.bak:772
#, c-format
msgid "\n"
"LimbCorrectionMoon = %.4f\n"
msgstr "\n"
"LimbCorrectionMoon = %.4f\n"

#: src\Sight.cpp:774 src\Sight.cpp.bak:775
#, c-format
msgid "\n"
"Corrected Altitude\n"
"CorrectedAltitudeMoon = ApparentAltitudeMoon - RefractionCorrectionMoon - LimbCorrectionMoon\n"
"CorrectedAltitudeMoon = %.4f - %.4f - %.4f\n"
"CorrectedAltitudeMoon = %.4f\n"
msgstr "\n"
"Διορθωμένο υψόμετρο\n"
"Διορθωμένο Υψόμετρο Σελήνης = Φαινόμενο Υψόμετρο - Διόρθωση Διάθλασης - Διόρθωση ακτίνας\n"
"Διορθωμένο Υψόμετρο Σελήνης = %.4f - %.4f - %.4f\n"
"Διορθωμένο Υψόμετρο Σελήνης = %.4f\n"

#: src\Sight.cpp:789 src\Sight.cpp.bak:790
#, c-format
msgid "ParallaxCorrectionMoon = -180/Pi * asin( sin(Pi/180 * HP ) * cos(Pi/180 * CorrectedAltitude))\n"
"ParallaxCorrectionMoon = -180/Pi * asin( sin(Pi/180 * %.4f ) * cos(Pi/180 * %.4f))\n"
"ParallaxCorrectionMoon = %.4f\n"
msgstr "Διόρθωση Παράλλαξης Σελήνης = -180/π * asin( sin(π/180 * HP ) * cos(π/180 * ΔιορθωμένοΥψόμετρο))\n"
"ΔιόρθωσηΠαράλλαξης Σελήνης = -180/π * asin( sin(π/180 * %.4f ) * cos(π/180 * %.4f)) = %.4f\n"

#: src\Sight.cpp:878 src\Sight.cpp.bak:879
#, c-format
msgid "CorrectionsMoon = %.4f\n"
"CorrectionsBody = %.4f\n"
"Corrections = abs(CorrectionsMoon - CorrectionsBody) = %.4f\n"
msgstr "Διορθώσεις Σελήνης = %.4f\n"
"Σώμα Διορθώσεων = %.4f\n"
"Corrections = abs(Διορθώσεις Σελήνης - Σώμα Διορθώσεων) = %.4f\n"

#: src\Sight.cpp:884 src\Sight.cpp.bak:885
#, c-format
msgid "CorrectedMeasurement = Measurement - Corrections - IndexCorrection\n"
"CorrectedMeasurement = %.4f - %.4f - %.4f\n"
